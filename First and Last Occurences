
class Solution:
    def find(self, arr, x):
        
        # code here
         
        first = -1
        last = -1
        for i in range(len(arr)):
            if arr[i] == x:
                first = i
                break
        for i in range(len(arr)):
            if arr[i]==x:
                last = max(last,i)
        return [first, last]
